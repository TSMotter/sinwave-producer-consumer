# Doxyfile

# Disables output messages
QUIET = YES

# Specify the source code directory to be documented
INPUT = @CMAKE_SOURCE_DIR@/lib \
        @CMAKE_SOURCE_DIR@/readme.md

# Specify the output directory for the generated documentation
OUTPUT_DIRECTORY = @CMAKE_CURRENT_BINARY_DIR@

# Strip a user-defined part of the path.
STRIP_FROM_PATH = @CMAKE_SOURCE_DIR@

# Set the project name and version
PROJECT_NAME = "@CMAKE_PROJECT_NAME@"
PROJECT_NUMBER = 1.0

# Set the desired output format
GENERATE_HTML = YES

# Include only relevant files
FILE_PATTERNS = *.cpp *.hpp

# Exclude files or directories if necessary
EXCLUDE_PATTERNS =


# Explicitly disable the usage of Graphviz
HAVE_DOT = NO

# Explicitly controls what output is desired
GENERATE_HTML = YES
GENERATE_LATEX = NO
GENERATE_MAN = NO
GENERATE_RTF = NO
GENERATE_XML = NO
GENERATE_DOCBOOK = NO
GENERATE_HTMLHELP = NO
GENERATE_QHP = NO
GENERATE_ECLIPSEHELP = NO
GENERATE_DOCSET = NO
USE_PDFLATEX = NO